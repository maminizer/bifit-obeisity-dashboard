{% extends 'base.html.twig' %}

{#% block title %}
	Obesity Prediction
{% endblock %}

{% block body %}
	<div id="contact" class="text-center">
		<div class="container">

			<div class="section-title text-center">
				<h2>
					Log In
				</h2>
				<hr>
				<p>
					Lorem ipsum dolor sit amet, consectetur adipiscing elit duis sed.
				</p>
			</div>
			<div class="col-md-10 col-md-offset-1">
				<form method="post">
					<div class="row">
						<div class="col-md-6">
							<div class="form-group">
								<table class="table table-striped form-horizontal" style="margin-top: 1%">
									{{ form_start(form) }}
									<tr>
										<td>
											{{ form_label(form.child,"Child") }}
										</td>
										<td>
											{{ form_widget(form.child) }}
										</td>
									</tr>

									<tr>
										<td>
											{{ form_label(form.farm,"Farmers Markets") }}
										</td>
										<td>
											{{ form_widget(form.farm) }}
										</td>
									</tr>
									<tr>
										<td>
											{{ form_label(form.recfa,"Recreation Facilities") }}
										</td>
										<td>
											{{ form_widget(form.recfa) }}
										</td>
									</tr>
									<tr>
										<td>
											{{ form_label(form.restaurent,"Restaurent") }}
										</td>
										<td>
											{{ form_widget(form.restaurent) }}
										</td>
									</tr>
									<tr>
										<td>
											{{ form_label(form.store,"Store") }}
										</td>
										<td>
											{{ form_widget(form.store) }}
										</td>
									</tr>
									<tr>
										<td>
											<buttontype="submit"class="btnbtn-theme04deleteform-control">
												Predict
											</button>
										</td>
									</tr>
								</table>
								{{form_end(form)}}
								<table class="table table-striped form-horizontal" style="margin-top:1%">
									<tr>
										<td>
											predictedobesity:
											{{obesity_rate}}
										</td>
									</tr>
									<tr>
										<td>
											Averageobesity:
											{{Average_obesity}}
										</td>
									</tr>
									<tr>
										<td>
											obesityevaluation:
											{{Obesity_evaluation}}
										</td>
									</tr>
								</table>
								{% if obesity_rate is  defined %}
								{% endif %}
							{% endblock %#}
